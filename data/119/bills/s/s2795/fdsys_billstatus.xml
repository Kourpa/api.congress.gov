        <title>Funding for Aviation Screeners and Threat Elimination Restoration Act</title>
        <updateDate>2025-09-23T04:23:15Z</updateDate>
        <billTextVersionName>Introduced in Senate</billTextVersionName>
        <billTextVersionCode>IS</billTextVersionCode>
