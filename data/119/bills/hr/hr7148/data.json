      ],
      "result": "pass",
      "roll": "45",
      "status": "PASS_OVER:HOUSE",
      "suspension": null,
      "text": "On passage Passed by the Yeas and Nays: 341 - 88 (Roll No. 45).",
      "type": "vote",
      "vote_type": "vote",
      "where": "h"
    },
    {
