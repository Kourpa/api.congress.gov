      <item>
        <titleType>Short Title(s) as Introduced</titleType>
        <titleTypeCode>101</titleTypeCode>
        <title>Cancer Drug Parity Act of 2025</title>
        <updateDate>2025-07-08T09:23:19Z</updateDate>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
      </item>
      <item>
        <titleType>Official Title as Introduced</titleType>
        <titleTypeCode>6</titleTypeCode>
        <title>To amend the Employee Retirement Income Security Act of 1974 to require a group health plan (or health insurance coverage offered in connection with such a plan) to provide for cost-sharing for oral anticancer drugs on terms no less favorable than the cost-sharing provided for anticancer medications administered by a health care provider.</title>
        <updateDate>2025-07-08T09:18:31Z</updateDate>
        <billTextVersionName>Introduced in House</billTextVersionName>
        <billTextVersionCode>IH</billTextVersionCode>
      </item>
